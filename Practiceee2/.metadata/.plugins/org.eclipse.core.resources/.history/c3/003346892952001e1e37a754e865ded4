
public class Student {

	 private String name;
	 int yearOfStudy;
	 int counter;
	int id;
	
	public Student() {
		id = counter++;
	}
	public Student(String name, int yearOfStudy) {
		this( );
		name = name;
		yearOfStudy = yearOfStudy;
		
	}
	public String toString() {
		return this.name+ " " + yearOfStudy;
	}
	public String getName() {
		return this.name;
	}
	public void setName(String newName) {
		this.name = newName;
	}
}
